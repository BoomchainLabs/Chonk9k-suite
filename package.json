{
  "name": "earn-app",
  "version": "0.0.1-alpha.1",
  "description": "The official TypeScript library for the Earn App API",
  "author": "Earn App <cashapppaymentpay01@gmail.com>",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "type": "commonjs",
  "repository": "github:BoomchainLabs/Chonk9k-suite",
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "files": [
    "**/*"
  ],
  "private": false,
  "scripts": {
    "test": "./scripts/test",
    "build": "./scripts/build && npm run sentry:sourcemaps",
    "prepublishOnly": "echo 'to publish, run yarn build && (cd dist; yarn publish)' && exit 1",
    "format": "./scripts/format",
    "prepare": "if ./scripts/utils/check-is-in-git-install.sh; then ./scripts/build && ./scripts/utils/git-swap.sh; fi",
    "tsn": "ts-node -r tsconfig-paths/register",
    "lint": "./scripts/lint",
    "fix": "./scripts/format",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org boomchainlab --project javascript-vue dist && sentry-cli sourcemaps upload --org boomchainlab --project javascript-vue dist"
  },
  "dependencies": {
    "@sentry/cli": "^2.51.1",
    "@sentry/rollup-plugin": "^4.1.0",
    "@sentry/vue": "^10.4.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "expo": "^53.0.20",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.0",
    "@swc/core": "^1.3.102",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.20.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "iconv-lite": "^0.6.3",
    "jest": "^29.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "publint": "^0.2.12",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.5.0",
    "tsc-multi": "https://github.com/stainless-api/tsc-multi/releases/download/v1.1.7/tsc-multi.tgz",
    "tsconfig-paths": "^4.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1"
  },
  "imports": {
    "earn-app": ".",
    "earn-app/*": "./src/*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*.mjs": {
      "default": "./dist/*.mjs"
    },
    "./*.js": {
      "default": "./dist/*.js"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  }
}
